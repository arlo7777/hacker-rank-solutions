# Practice / Python / Basic Data Types / Introduction to Sets

def average(array):
    # your code goes here
    set_array = set(array)
    average = sum(set_array)/len(set_array)
    return average 
    
if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().split()))
    result = average(arr)
    print(result)
